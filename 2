#include <iostream>
#include <cmath>
#include <iomanip>
using namespace std;
void fout (double x1,double y,int k,double g)
{
    cout << x1 << setprecision(4) << " " << y << " " << k << " " << g << endl;
}
void function (int i, int j, int k, double x1, double x2, double dx, double e, double g, double y, double u, double m, double v)
{
    g=0;
    k=0;
    for (i = 0; x1 <= x2; i++)
    {
        y = (1. / 2) * log((x1 + 1) / (x1 - 1));
        for (j = 0; j < 3; j++) {
            m = -2 * j - 1;
            u = pow(x1, m);
            v = 2 * j + 1;
            g += u / v;
            k+=1;
        }
        fout (x1, y, k, g);
        g = 0;
        k=0;
        x1 += dx;
    }
}
int main()
{
    int i,j,k;
    double x1, x2, dx, e, g = 0, y, u, m, v;
    cin >> x1 >> x2 >> dx >> e;
    function(i,j,k,x1,x2,dx,e,g,y,u,m,v);
    return 0;
}
