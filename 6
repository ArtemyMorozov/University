    };
    char inpstr[80];
    cin.getline(inpstr, 80);
    col = strlen(inpstr);
    int* arr2 = new int[col];
    for (int k = 0; k < col; k++)
        arr2[k] = 0;
    A = 2;
    for (i = 0; i < col; i = i + 1)
    {
        switch (inpstr[i])
        {
            case '0':  B = array[A][0]; break;
            case '1':  B = array[A][1]; break;
            case '2':  B = array[A][2]; break;
            case '3':  B = array[A][3]; break;
            case '4':  B = array[A][4]; break;
            case '5':  B = array[A][5]; break;
