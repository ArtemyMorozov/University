            case '8':  B = array[A][8]; break;
            case '9':  B = array[A][9]; break;
            case '{':  B = array[A][10]; break;
            case '}':  B = array[A][11]; break;
            case '.':  B = array[A][12]; break;
            case ',':  B = array[A][13]; break;
            case ';':  B = array[7][A]; break;
            case ' ':  B = A; break;
            default: B = 1;
        }
        A = B;
    };
    if(B==1)
        cout << "STRING is WRONG";
    else if(B==0)
        cout << "STRING is RIGHT";
    delete[] arr2;
    return 0;
};
