#include <iostream>
#include "math.h"
using namespace std;
int main()
{
    double F, a, b, c, x1, x2, dx,i;
    cin >> x1 >> x2 >> dx >> a >> b >> c;
    i = 0;
    F=0;
    while (x1 <= x2)
    {
        if ((x1 < 1) && (c != 0))
        {
            F = (x1*x1)*a + b / c;
        }
        else if ((x1 > 1.5) && (c == 0))
        {
            F = (x1 - a) / ((x1 - c)*(x1 - c));
        }
        else
        {
            F = (x1*x1) / (c*c);
        }
        i = i + 1;
        cout << i << ' ' << x1 << ' ' << F << '\n';
        x1 = x1 + dx;
    }
    return 0;
}
