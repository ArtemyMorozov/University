#include <iostream>
#include <cmath>
using namespace std;

class error404{ };

double proverka(double A1, double B1, double A2, double B2)
{
    if (A1==A2 && B1==B2)
        throw error404();
    double c = A1-A2;
    double d = B1-B2;
    return sqrt((pow(c,2)+pow(d,2)));
}
int main()
{
    setlocale(LC_ALL, "Russian");
    int i,n;
    double a1,a2,b1,b2;
    cin >> n;
    for (i=0; i<n; i++) {
        cin >> a1 >> b1 >> a2 >> b2;
        try
        {
            double c = proverka(a1, b1, a2, b2);
            cout << c << endl;
        }
        catch (error404)
        {
            cout << "Error â„– 1  - tochki sovpadaut!!!!" << endl;
        }
    }
    return 0;
}
