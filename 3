#include <iostream>
using namespace std;
int main()
{
    int min_index;
    int n;
    cin >> n;
    int* M = new int[n];
    float min;
    int i;
    for (i = 0; i <= n - 1; i++)
    {
        cin >> M[i];
    }
    min = abs(M[0]);
    for (i = 0; i <= n - 1; i++)
    {
        if (abs(M[i]) < min)
        {
            min = abs(M[i]);
            min_index = i;
        }
    }
    cout << min_index + 1;
    float sum = 0.0;
    bool fn = false;
    for (i = 0; i <= n - 1; i++)
    {
        if ((M[i] < 0) && (!fn))
        {
            fn = true;
        }
        else if (fn)
        {
            sum += abs(M[i]);
        }
    }
    cout << ' ' << sum << '\n';
    float a, b;
    int k = 0;
    cin >> a >> b;
    int* M1 = new int[n];
    for (i = 0; i <= n - 1; i++)
    {
        if ((abs(M[i]) < a) || (abs(M[i]) > b))
        {
            M1[k] = M[i];
            k++;
        }
    }for (i = k; i <= n - 1; i++)
    {
        M1[i] = 0;
    }
    for (i = 0; i <= n - 1; i++)
    {
        cout << M1[i] << ' ';
    }
    return 0;
}
